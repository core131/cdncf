{
  "name": "vlcdn",
  "version": "1.0.0",
  "description": "VLESS VPN Generator dengan Cloudflare Workers dan KV Storage",
  "main": "worker.js",
  "type": "module",
  "scripts": {
    "dev": "wrangler dev",
    "deploy": "wrangler deploy",
    "publish": "wrangler deploy",
    "start": "wrangler dev",
    "tail": "wrangler tail",
    "kv:create": "wrangler kv:namespace create VLESS_ACCOUNTS",
    "kv:list": "wrangler kv:namespace list",
    "kv:get": "wrangler kv:key get --binding=VLESS_ACCOUNTS",
    "kv:put": "wrangler kv:key put --binding=VLESS_ACCOUNTS",
    "kv:delete": "wrangler kv:key delete --binding=VLESS_ACCOUNTS",
    "kv:list-keys": "wrangler kv:key list --binding=VLESS_ACCOUNTS",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "format": "prettier --write .",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "vless",
    "vpn",
    "cloudflare",
    "workers",
    "websocket",
    "cdn",
    "proxy",
    "v2ray",
    "xray",
    "kv-storage"
  ],
  "author": "Your Name",
  "license": "MIT",
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20241022.0",
    "@cloudflare/vitest-pool-workers": "^0.5.4",
    "@types/node": "^20.10.5",
    "eslint": "^8.56.0",
    "prettier": "^3.1.1",
    "typescript": "^5.3.3",
    "vitest": "^2.1.3",
    "wrangler": "^3.82.0"
  },
  "dependencies": {
    "@cloudflare/kv-asset-handler": "^0.3.4"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/vless-vpn-cloudflare-workers.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/vless-vpn-cloudflare-workers/issues"
  },
  "homepage": "https://github.com/yourusername/vless-vpn-cloudflare-workers#readme"
}
